{
    "scripts": {
        "start": "elm-reactor",
        "clean:src": "node -e \"var d='elm-stuff',fs=require('fs-extra');fs.emptydir(d);\"",
        "clean:test": "node -e \"var d='tests/elm-stuff',fs=require('fs-extra');fs.emptydir(d);\"",
        "clean:build": "node -e \"var d='_build',fs=require('fs-extra');fs.emptydir(d);\"",
        "clean": "run-p clean:*"
    },
    "devDependencies": {
        "elm": "^0.17.1",
        "fs-extra": "^0.30.0"
    }
}
